{
	"_version": "1.7.0",
	"sap.app": {
		"id": "Subash1Subash1",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZODATA_SHOP_PLAN_SRV_02": {
				"uri": "/sap/opu/odata/sap/ZODATA_SHOP_PLAN_SRV_02/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/ZODATA_SHOP_PLAN_SRV_02/metadata.xml"
				}
			},
			"ZODATA_EHSM_LOGIN_SRV": {
				"uri": "/sap/opu/odata/sap/ZODATA_EHSM_LOGIN_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/ZODATA_EHSM_LOGIN_SRV/metadata.xml"
				}
			},
			"ZODATA_SHOP_PRODUCTION_SRV_01": {
				"uri": "/sap/opu/odata/sap/ZODATA_SHOP_PRODUCTION_SRV_01/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/ZODATA_SHOP_PRODUCTION_SRV_01/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "Subash1Subash1.view.View1",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "Subash1Subash1.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"viewPath": "Subash1Subash1.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"routerClass": "sap.m.routing.Router",
				"clearControlAggregation": false,
				"viewLevel": 1,
				"async": true
			},
			"targets": {
				"View1": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "View1",
					"viewLevel": 1
				},
				"View2": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "View2"
				},
				"Production": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Production"
				},
				"Dashboard": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Dashboard"
				}
			},
			"routes": [
				{
					"name": "View1",
					"pattern": "View1",
					"titleTarget": "",
					"greedy": false,
					"target": [
						"View1"
					]
				},
				{
					"name": "View2",
					"pattern": "View2",
					"titleTarget": "",
					"greedy": false,
					"target": [
						"View2"
					]
				},
				{
					"name": "Production",
					"pattern": "Production",
					"titleTarget": "",
					"greedy": false,
					"target": [
						"Production"
					]
				},
				{
					"name": "Dashboard",
					"pattern": "Dashboard",
					"titleTarget": "",
					"greedy": false,
					"target": [
						"Dashboard"
					]
				}
			]
		}
	}
}